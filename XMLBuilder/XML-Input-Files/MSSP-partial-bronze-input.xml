<config version="7.1.0" urldb="paloaltonetworks">
  <devices>
    <entry name="localhost.localdomain">
      <vsys>
        <entry name="vsys1">
          <rulebase>
            <security>
              <rules>
                <entry name="MSSP-allow-outbound">
                  <to>
                    <member>$ZONE_UNTRUST</member>
                  </to>
                  <from>
                    <member>$ZONE_TRUST</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>application-default</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                  <log-setting>MSSP-logging</log-setting>
                  <profile-setting>
                    <group>
                      <member>MSSP-Bronze</member>
                    </group>
                  </profile-setting>
                  <tag>
                    <member>$PANOS</member>
                    <member>$TEMPVER</member>
                    <member>Bronze</member>
                  </tag>
                  <description>This is a general rule to allow trust to untrust traffic. It uses application defaults to prevent applications from running on non-default undefined ports. This is part of the App-ID support in the firewall</description>
                </entry>
                <entry name="MSSP-non-def-SSL-ports">
                  <to>
                    <member>$ZONE_UNTRUST</member>
                  </to>
                  <from>
                    <member>$ZONE_TRUST</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>ssl</member>
                  </application>
                  <service>
                    <member>MSSP-SSL-non-default-ports</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                  <log-start>yes</log-start>
                  <log-end>yes</log-end>
                  <log-setting>MSSP-logging</log-setting>
                  <profile-setting>
                    <group>
                      <member>MSSP-Bronze</member>
                    </group>
                  </profile-setting>
                  <tag>
                    <member>$PANOS</member>
                    <member>$TEMPVER</member>
                    <member>Bronze</member>
                  </tag>
                  <description>This is a complement to the allow-outbound rule to ensure sessions can be established and the proper application determined. Once determined, the session will shift to the allow-outbound rule.</description>
                </entry>
                <entry name="MSSP-dyn-STUN-ports">
                  <to>
                    <member>$ZONE_UNTRUST</member>
                  </to>
                  <from>
                    <member>$ZONE_TRUST</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>stun</member>
                  </application>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                  <log-setting>MSSP-logging</log-setting>
                  <profile-setting>
                    <group>
                      <member>MSSP-Bronze</member>
                    </group>
                  </profile-setting>
                  <tag>
                    <member>$PANOS</member>
                    <member>$TEMPVER</member>
                    <member>Bronze</member>
                  </tag>
                  <description>Allow STUN-based NAT traversal applications that don't have preset default application ports.</description>
                </entry>
                <entry name="MSSP-find-non-def-apps">
                  <to>
                    <member>$ZONE_UNTRUST</member>
                  </to>
                  <from>
                    <member>$ZONE_TRUST</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                  <log-start>yes</log-start>
                  <profile-setting>
                    <group>
                      <member>MSSP-Bronze</member>
                    </group>
                  </profile-setting>
                  <tag>
                    <member>$PANOS</member>
                    <member>$TEMPVER</member>
                    <member>Bronze</member>
                  </tag>
                  <description>This is a temporary safety net rule to find apps not on non-default ports. It still has the Bronze security profile to look for threats, block bad URL's, and send files to WF.</description>
                  <log-setting>MSSP-logging</log-setting>
                </entry>
              </rules>
            </security>
          </rulebase>
          <profile-group>
            <entry name="MSSP-Bronze">
              <file-blocking>
                <member>MSSP</member>
              </file-blocking>
            </entry>
          </profile-group>
        </entry>
      </vsys>
    </entry>
  </devices>
</config>
